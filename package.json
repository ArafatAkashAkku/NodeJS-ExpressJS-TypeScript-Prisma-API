{
  "name": "backend-nodejs",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/server.js",
    "sql": "npx prisma migrate dev --name init",
    "nosql": "npx prisma db push",
    "generate": "npx prisma generate",
    "update": "ncu -u && rimraf node_modules package-lock.json && npm install",
    "optionalupdate": "ncu -u && (if exist node_modules rmdir /s /q node_modules) && (if exist package-lock.json del /f /q package-lock.json) && npm install",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "lint": "eslint \"src/**/*.{ts,js}\" --fix",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "axios": "^1.9.0",
    "body-parser": "^2.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-status-monitor": "^1.3.4",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.3",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.2",
    "@types/express-status-monitor": "^1.3.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.23",
    "@types/node-cache": "^4.2.5",
    "@types/nodemailer": "^6.4.17",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "nodemon": "^3.1.10",
    "npm-check-updates": "^18.0.1",
    "prettier": "^3.5.3",
    "prisma": "^6.8.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0"
  },
  "lint-staged": {
    "src/**/*.{ts,js,json,md}": [
      "prettier --write",
      "eslint --fix",
      "git add ."
    ]
  }
}
